{
  "name": "scru160",
  "version": "0.2.2",
  "description": "SCRU-160: Sortable, Clock and Random number-based Unique identifier",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./esm.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "dist",
    "esm.mjs"
  ],
  "sideEffects": false,
  "browser": {
    "crypto": "./dist/dummy_crypto.js"
  },
  "scripts": {
    "build": "tsc && webpack ./test/*.mjs -o ./test/bundle --mode production",
    "doc": "typedoc ./src/index.ts --gitRevision \"v$npm_package_version\"",
    "prebuild": "rm -rf ./dist ./test/bundle",
    "predoc": "rm -rf ./docs",
    "prepare": "npm run build",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scru160/javascript.git"
  },
  "keywords": [
    "identifier",
    "uuid",
    "guid",
    "ulid",
    "ksuid"
  ],
  "author": "LiosK <contact@mail.liosk.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/scru160/javascript/issues"
  },
  "homepage": "https://github.com/scru160/javascript#readme",
  "devDependencies": {
    "@types/node": "^16.9.2",
    "mocha": "^9.1.1",
    "typedoc": "^0.22.3",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  }
}
